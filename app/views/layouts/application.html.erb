<!DOCTYPE html>
<html>
  <head>
    <title>KZXT</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all' %>
    <%= javascript_include_tag 'application' %>
  </head>

  <body>
    <script type="text/javascript">
      window.homePhotoURL = "<%= image_url('home-photo.png') %>";
      window.logoURL = "<%= image_url('logo.png') %>";
      window.logoblkURL = "<%= image_url('logo-black.png') %>";
      window.catURL = "<%= image_url('Cat.jpg') %>";
    </script>
    <%= yield %>
  </body>
</html>
